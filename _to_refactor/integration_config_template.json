{
    "analyzers": {
        "zanflow": {
            "enabled": true,
            "module_path": "./zanflow_microstructure_analyzer.py",
            "weight": 0.3,
            "config": {
                "lookback_period": 100,
                "min_volume_threshold": 1000
            }
        },
        "ncos": {
            "enabled": true,
            "module_path": "./ncOS_ultimate_microstructure_analyzer.py",
            "weight": 0.4,
            "config": {
                "wyckoff_sensitivity": 0.7,
                "smc_threshold": 0.6
            }
        },
        "smc": {
            "enabled": true,
            "module_path": "./convert_final_enhanced_smc_ULTIMATE.py",
            "weight": 0.3,
            "config": {
                "order_block_sensitivity": 0.8,
                "fvg_minimum_size": 0.001
            }
        }
    },
    "results_dir": "./integrated_results",
    "max_workers": 4,
    "consensus_threshold": 0.6,
    "output_formats": ["json", "csv", "parquet"],
    "batch_processing": {
        "enabled": true,
        "chunk_size": 10,
        "parallel_files": 3
    },
    "signal_filters": {
        "min_confidence": 0.5,
        "max_age_minutes": 60,
        "require_consensus": true
    }
}